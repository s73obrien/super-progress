{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Test",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "test"
      ],
      "port": 9229,
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Javascript Example",
      "program": "${workspaceFolder}\\dist\\demos\\example.js",
      "console": "integratedTerminal",
      "sourceMaps": false,
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Typescript Example",
      "preLaunchTask": "build",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "program": "${workspaceFolder}\\src\\examples\\examples.ts",
      "outFiles": [
        "${workspaceFolder}\\dist\\**\\*"
      ],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "protocol": "inspector"
    }
  ]
}